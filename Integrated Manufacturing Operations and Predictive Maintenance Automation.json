      "parameters": {
        "mode": "runOnceForEachItem",
        "jsCode": "// Validate sensor data\nconst item = $input.item.json;\n\n// Define required fields\nconst requiredFields = ['machineId', 'temperature', 'vibration', 'pressure', 'timestamp'];\n\n// Check for missing required fields\nconst missingFields = requiredFields.filter(field => !(field in item));\nif (missingFields.length > 0) {\n  throw new Error(`Missing required fields: ${missingFields.join(', ')}`);\n}\n\n// Validate machineId (should be a non-empty string)\nif (typeof item.machineId !== 'string' || item.machineId.trim() === '') {\n  throw new Error('machineId must be a non-empty string');\n}\n\n// Validate temperature (should be a number between -50 and 200 degrees Celsius)\nif (typeof item.temperature !== 'number' || isNaN(item.temperature)) {\n  throw new Error('temperature must be a valid number');\n}\nif (item.temperature < -50 || item.temperature > 200) {\n  throw new Error('temperature must be between -50 and 200 degrees Celsius');\n}\n\n// Validate vibration (should be a number between 0 and 100 mm/s)\nif (typeof item.vibration !== 'number' || isNaN(item.vibration)) {\n  throw new Error('vibration must be a valid number');\n}\nif (item.vibration < 0 || item.vibration > 100) {\n  throw new Error('vibration must be between 0 and 100 mm/s');\n}\n\n// Validate pressure (should be a number between 0 and 1000 PSI)\nif (typeof item.pressure !== 'number' || isNaN(item.pressure)) {\n  throw new Error('pressure must be a valid number');\n}\nif (item.pressure < 0 || item.pressure > 1000) {\n  throw new Error('pressure must be between 0 and 1000 PSI');\n}\n\n// Validate timestamp (should be a valid date string or timestamp)\nconst timestamp = new Date(item.timestamp);\nif (isNaN(timestamp.getTime())) {\n  throw new Error('timestamp must be a valid date');\n}\n\n// Return validated data\nreturn {\n  json: {\n    machineId: item.machineId,\n    temperature: item.temperature,\n    vibration: item.vibration,\n    pressure: item.pressure,\n    timestamp: item.timestamp,\n    validatedAt: new Date().toISOString(),\n    status: 'valid'\n  }\n};"
      },
